interface wlan0
max_leases 50
start 192.168.1.51
end 192.168.1.101
decline_time 3600
conflict_time 3600
offer_time 60
min_lease 60
lease_file /tmp/leases.lan
remaining yes
auto_time 300
opt domain localdomain
opt dns 192.168.1.1
opt router 192.168.1.1
opt subnet 255.255.255.0
opt ntpsrv 192.168.1.1
opt lease 86400
